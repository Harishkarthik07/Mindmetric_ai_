# Environment variables for MindMetric AI
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=sqlite:///mindmetric.db

# Session Security
SESSION_SECRET=your-secret-key-here-change-this-in-production

# Google Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key-here

# Optional: PostgreSQL Database (if using instead of SQLite)
# PGHOST=localhost
# PGPORT=5432
# PGDATABASE=mindmetric
# PGUSER=your-username
# PGPASSWORD=your-password

# Application Settings
FLASK_ENV=development
FLASK_DEBUG=True
